package com.company;

public class fibonacci_series {
    public static void main(String[] args) {
        int a=0,b=1,c;
        System.out.println();
        System.out.println("\t\t\t!!!!!FIBONACCI SERIES!!!!!");
        System.out.println();
        System.out.print(a+ " " + b);
        for(int i=1;i<=17;i++){
            c=a+b;
            System.out.print(" " +c);
            a=b;
            b=c;
        }
        System.out.println();
    }
}
